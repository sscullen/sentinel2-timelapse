{
  "name": "reactexample",
  "version": "1.0.0",
  "description": "basic reactJS ",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp client-src/index.html client-dist/index.html && webpack-dev-server --content-base client-dist/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p && cp client-src/index.html client-dist/index.html",
    "server": "nodemon --abort-on-uncaught-exception server-src/server.js"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Shaun Cullen",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.147.0",
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "coordinator": "^0.5.0",
    "express": "^4.16.2",
    "express-rate-limit": "^2.9.0",
    "leaflet": "^1.2.0",
    "leaflet-draw": "^0.4.12",
    "leaflet.utm": "0.0.4",
    "multer": "^1.3.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-leaflet": "^1.7.3",
    "react-leaflet-draw": "^0.16.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^2.10.2",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "utm": "^1.1.1",
    "utm-latlng": "^1.0.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.9.1"
  }
}
