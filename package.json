{
  "name": "reactexample",
  "version": "1.0.0",
  "description": "basic reactJS ",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp client-src/index.html client-dist/index.html && webpack-dev-server --content-base client-src/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p && cp client-src/index.html client-dist/index.html",
    "server": "nodemon server-src/server.js"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Shaun Cullen",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.90.0",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "coordinator": "^0.5.0",
    "express": "^4.15.3",
    "express-rate-limit": "^2.8.1",
    "leaflet": "^1.0.3",
    "leaflet-draw": "^0.4.10",
    "multer": "^1.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-leaflet": "^1.3.1",
    "react-leaflet-draw": "^0.16.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-promise-middleware": "^4.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
